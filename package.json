{
  "dependencies": {
    "@create-figma-plugin/ui": "^2.5.1",
    "@create-figma-plugin/utilities": "^2.5.1",
    "preact": "^10.16.0"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^2.5.1",
    "@create-figma-plugin/tsconfig": "^2.5.1",
    "@figma/plugin-typings": "1.70.0",
    "@tabler/icons": "^2.43.0",
    "fuse.js": "^6.6.2",
    "typescript": "^5"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch",
    "icons": "ncu -u @tabler/icons && pnpm install && node import-icons && pnpm run build && git add . && git commit -am \"update icons to v`pnpm info @tabler/icons version`\" && git push"
  },
  "figma-plugin": {
    "editorType": [
      "figma",
      "figjam"
    ],
    "id": "1169807996149376642",
    "name": "Tabler Icons",
    "main": "src/main.ts",
    "ui": "src/ui.tsx",
    "networkAccess": {
      "allowedDomains": [
        "none"
      ],
      "reasoning": "This plugin does not require network access."
    }
  }
}
